{
  "name": "parallelcode",
  "version": "1.0.0",
  "description": "Execute code in a Node.js worker threads pool for fast and parallel execution time",
  "main": "index.js",
  "scripts": {
    "test": "node  --experimental-worker ./node_modules/jest/bin/jest.js",
    "coverage": "node  --experimental-worker ./node_modules/jest/bin/jest.js --coverage"
  },
  "keywords": [
    "worker",
    "thread",
    "pool",
    "job",
    "parallel",
    "code"
  ],
  "author": "manuel-di-iorio",
  "license": "MIT",
  "devDependencies": {
    "jest": "^24.9.0",
    "semistandard": "^14.1.0"
  },
  "engines": {
    "node": ">10.5"
  },
  "semistandard": {
    "ignore": [
      "/tests/"
    ]
  }
}
